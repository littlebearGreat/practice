page{
  height: 100%;
}
.body{
  width: 100%;
  min-height: 100%;
  background-color: #f5f5f5;
  overflow: hidden;
}
.control,
.state,
.parameter{
  width: 690rpx;
  margin: 22rpx auto 0;
  overflow: hidden;
  background-color: #fff;
  border-radius: 8rpx;
}
.control .img{
  float: left;
  width: 280rpx;
  height: 280rpx;
  margin-left: 30rpx;
  vertical-align: top;
  margin-top: 30rpx;
  margin-bottom: 30rpx;
}
.control .img image{
  width: 100%;
  height: 100%;
}
.control .controlBtns{
  float: right;
  margin-top: 30rpx;
  overflow: hidden;
  width: 336rpx;
  margin-bottom: 30rpx;
}
.control .controlBtns view{
  float: left;
  margin-right: 30rpx;
  margin-bottom: 20rpx;
  width: 80rpx;
  height: 80rpx;
  background-color: #ddd;
  border-radius: 8rpx;
  overflow: hidden;
}
.control .controlBtns text{
  display: block;
  color: #666;
  font-size: 22rpx;
  line-height: 30rpx;
  letter-spacing: .2em;
  margin-left: 12rpx;
}
.control .controlBtns .top{
  margin-top: 10rpx;
}
.control .controlBtns view image{
  width: 28rpx;
  height: 28rpx;
  margin: 26rpx;
}
.control .controlBtns .active{
  background-color: #0377fa;
}
.control .controlBtns .active text{
  color: #fff;
}
/*运行状态*/
.state .h1{
  display: block;
  margin-top: 10rpx;
  font-size: 30rpx;
  line-height: 70rpx;
  margin-left: 30rpx;
  color: #333;
}
.state .list{
  display: flex;
  flex-wrap:wrap;
  justify-content:space-between;
  overflow: hidden;
  margin-left: 30rpx;
  margin-right: 30rpx;
  margin-bottom: 20rpx;
}
.state .list view{
  display: inline-block;
  width: 300rpx;
  font-size: 24rpx;
  line-height: 64rpx;
  color: #666;
  border-bottom: 1px solid #ddd;
}