/**index.wxss**/
.search{
  width: 690rpx;
  margin: 16rpx auto 15rpx;
  border-radius: 8rpx;
  background-color: #f0f0f0;
}
.search .searchIpt{
  width: 570rpx;
  height: 60rpx;
  display: inline-block;
  border-radius: 4rpx;
  vertical-align: top;
  padding: 0 20rpx;
  font-size: 28rpx;
  line-height: 60rpx;
}
.search image{
  display: inline-block;
  width: 26rpx;
  height: 26rpx;
  margin: 17rpx 22rpx;
  vertical-align: top;
}
.list{
  width: 100%;
  background-color: #f5f5f5;
  padding-bottom: 40rpx;
  overflow: hidden;
}
/*模板样式*/
.template{
  width: 690rpx;
  height: 240rpx;
  border-radius: 8px;
  background-color: #fff;
  margin: 20rpx auto 0;
}
.template .pic{
  float: left;
  vertical-align: top;
  width: 240rpx;
  height: 180rpx;
  border-radius: 4rpx;
  overflow: hidden;
  margin-top: 30rpx;
  margin-left: 30rpx;
}
.template .pic navigator{
  display: block;
  width: 100%;
  height: 100%;
}
.template .pic image{
  width: 100%;
  height: 100%;
}
.template .inf{
  float: right;
  vertical-align: top;
  width: 390rpx;
  margin-top: 20rpx;
}
.template .inf .tit{
  font-size: 30rpx;
  color: #333;
  line-height: 70rpx;
  display: block;
}
.template .inf .ct{
  font-size: 26rpx;
  color: #333;
  line-height: 46rpx;
  display: block;
}